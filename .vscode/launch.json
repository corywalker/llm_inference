{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug llm_inference",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bazel-bin/llm_inference",
      "args": [
        "--model",
        "~/gemma-3-1b-it-qat-q4_0-gguf/gemma-3-1b-it-q4_0.gguf"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "bazel-build-debug"
    },
    {
      "name": "Debug llama.cpp",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/third_party/llama.cpp/build_dbg/bin/llama-eval-callback",
      "args": [
        "-m",
        "~/gemma-3-1b-it-qat-q4_0-gguf/gemma-3-1b-it-q4_0.gguf",
        "-p",
        "Hello",
        "-s",
        "123",
        "-v",
        "-n",
        "100",
        // llama-eval-callback does --no-cnv by default
        "--no-repack"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/third_party/llama.cpp",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "C/C++: Build llama.cpp eval-callback",
      "miDebuggerPath": "/usr/bin/gdb"
    }
  ]
}
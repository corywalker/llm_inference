{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug llm_inference",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bazel-bin/llm_inference",
      "args": [
        "--model",
        "~/gemma-3-1b-it-qat-q4_0-gguf/gemma-3-1b-it-q4_0.gguf"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "bazel-build-debug"
    },
  ]
}